{
    "name": "mazedlx/ignition-migrations",
    "description": "An igintion package to show your migrations and their status.",
    "keywords": [
        "laravel",
        "flare",
        "ignition"
    ],
    "homepage": "https://github.com/mazedlx/ignition-migrations",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Leo-Pernold",
            "email": "mazedlx@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "facade/ignition": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mazedlx\\MigrationsTab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mazedlx\\MigrationsTab\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mazedlx\\MigrationsTab\\TabServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
